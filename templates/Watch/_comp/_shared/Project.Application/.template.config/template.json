{
  "$schema": "http://json.schemastore.org/template",
  "author": "Samsung Electronics",
  "classifications": ["Tizen", "Watch"],
  "name": "Watch.Comp.Project.Application",
  "identity": "Watch.Comp.Project.Application",
  "shortName": "Watch.Comp.Project.Application",
  "tags": {
    "language": "C#",
    "type": "item",
    "wts.type": "composition",
    "wts.platform": "Watch",
    "wts.version": "1.0.0",
    "wts.compositionFilter": "wts.type == project & $projectType == Application"
  },
  "preferNameDirectory": true,
  "symbols": {
    "wts.projectName": {
      "type": "parameter",
      "replaces": "Param_ProjectName",
      "fileRename": "Param_ProjectName"
    },
    "wts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    }
  }
}
